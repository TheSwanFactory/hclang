{
  "extends": "tslint:recommended",
  "defaultSeverity": "warning",
  "rules" : {
    "array-type": false,
    "max-classes-per-file": 3,
    "max-line-length" : false,
    "no-console" : false,
    "no-string-literal" : false,
    "no-var-requires" : false,
    "only-arrow-functions" : false,
    "variable-name" : false
  }
}
