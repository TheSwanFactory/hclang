{
  "extends": "tslint:latest",
  "rules" : {
    "max-classes-per-file": 3,
    "array-type": false,
    "prefer-const": false,

    "trailing-comma": false,
    "callable-types": false,
    "interface-over-type-literal": false,
    "object-literal-sort-keys": false,

    "no-console" : false ,
    "no-var-requires" : false
  }
}
